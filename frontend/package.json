{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "css-loader": "^7.1.2",
    "env-cmd": "^10.1.0",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "postcss-loader": "^8.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "source-map-loader": "^5.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "choose-platform": "node choose-platform.js",
    "tailwind:build": "tailwindcss -i src/index.css -o src/style.css",
    "tailwind:watch": "tailwindcss -i src/index.css -o src/style.css --watch",
    "start-virtual-js": "WATCHPACK_POLLING=true react-scripts start",
    "build-virtual-js": "WATCHPACK_POLLING=true react-scripts build",
    "start-native-js": "react-scripts start",
    "build-native-js": "react-scripts build",
    "start": "npm run choose-platform && node -e \"const commands = require('./.platform-commands.json'); require('child_process').execSync(commands.start, { stdio: 'inherit' });\"",
    "build": "npm run choose-platform && node -e \"const commands = require('./.platform-commands.json'); require('child_process').execSync(commands.build, { stdio: 'inherit' });\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "leaflet": "^1.9.4",
    "leaflet-control-geocoder": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "osmtogeojson": "^2.2.12",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^2.1.0",
    "tailwindcss": "^3.4.3"
  }
}
